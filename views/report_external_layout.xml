<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="Web Assets Common" inherit_id="web.report_assets_common">
            <xpath expr="//link[last()]" position="after">
                <link rel="stylesheet" type="text/less" href="/website_layout_customize/static/src/less/style.less"/>
            </xpath>
        </template>
        <template id="external_layout_background_title" inherit_id="web.external_layout_background">
            <xpath expr="//img" position="after">
                <div class="float-left">
                    <span class="header-title" t-field="res_company.report_header_name"/>
                </div>
            </xpath>
        </template>
    </data>
</odoo>
