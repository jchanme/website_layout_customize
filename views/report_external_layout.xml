<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="report.assets.pdf" inherit_id="web.report_assets_pdf">
            <xpath expr="//link[last()]" position="after">
                <link rel="stylesheet" type="text/css" href="/website_layout_customize/static/src/css/style.css"/>
            </xpath>
        </template>
        <template id="external_layout_background_title" inherit_id="web.external_layout_background">
            <xpath expr="//img" position="after">
                <div class="pull-left">
                    <span style="font-size: 30px; white-space: nowrap; font-family: 'Prompt', sans-serif;" t-field="company.report_header_name"/>
                </div>
            </xpath>
        </template>
    </data>
</odoo>
