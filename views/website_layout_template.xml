<odoo>
	<data>
		<template id="Web Assets Inherited" inherit_id="web.assets_frontend">
			<xpath expr="//link[last()]" position="after">
				<link rel="stylesheet" type="text/less" href="/website_layout_customize/static/src/less/style.less"/>
			</xpath>
		</template>
		
		<template id="Main Frontend Layout Inherit" inherit_id="portal.frontend_layout">
			<xpath expr="//div[@class='navbar-header']" position="after">
				<div class="navbar-brand fadeshow">
					<span style="font-family: 'Microsoft Yi Baiti', 'Montserrat'; font-size: 25px; vertical-align: middle;" t-field="res_company.report_header_name"/>
				</div>
			</xpath>
		</template>
	</data>
</odoo>
